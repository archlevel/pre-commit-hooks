{
  "name": "pre-commit-hooks",
  "version": "0.1.8",
  "description": "use pre-commit ",
  "main": "index.js",
  "scripts": {
    "install": "node install.js",
    "uninstall": "node uninstall.js",
    "test": "echo \"Error: no test specified\" && exit 1",
    "resetDir": "rm -rf publish && mkdir -p publish",
    "copyFile": "cp -r LICENSE shell package.json README.md install.js uninstall.js vue-eslint.js .npmignore .gitignore .eslintignore .eslint.config.js ./publish",
    "release": "npm run resetDir && npm run copyFile && cd ./publish && npm run publish",
    "patch": "npm version patch && npm run release"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rockywu/pre-commit-hooks.git"
  },
  "keywords": [
    "git",
    "hooks"
  ],
  "author": "rockywu <wjl19890427@hotmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/rockywu/pre-commit-hooks/issues"
  },
  "homepage": "https://github.com/rockywu/pre-commit-hooks#readme",
  "dependencies": {
    "babel-eslint": "^8.2.1",
    "eslint": "^4.15.0",
    "eslint-codeframe-formatter": "^1.0.2",
    "eslint-config-standard": "^11.0.0-beta.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-node": "^5.2.1",
    "eslint-plugin-promise": "^3.6.0",
    "eslint-plugin-standard": "^3.0.1",
    "eslint-plugin-vue": "^4.2.0",
    "pre-commit": "^1.2.2",
    "vue-eslint-parser": "^2.0.2"
  }
}
